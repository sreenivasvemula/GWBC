<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  
  <xsd:element name="ContactCreatePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactDeletePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactEditPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="ContactEntity">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Company"/>
      <xsd:enumeration value="CompanyVendor"/>
      <xsd:enumeration value="Contact"/>
      <xsd:enumeration value="LegalVenue"/>
      <xsd:enumeration value="Person"/>
      <xsd:enumeration value="PersonVendor"/>
      <xsd:enumeration value="Place"/>
      <xsd:enumeration value="UserContact"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <!-- Contact-related permissions -->
  <xsd:element name="ContactPermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ContactSubtypeAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactTagAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactSubtypeAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ContactCreatePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactDeletePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactEditPermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactViewPermission" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="entity" type="ContactEntity" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="ContactTag">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Claim Party"/>
      <!-- Client -->
      <xsd:enumeration value="Client"/>
      <!-- Vendor -->
      <xsd:enumeration value="Vendor"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:element name="ContactTagAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ContactCreatePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactDeletePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactEditPermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactViewPermission" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="tag" type="ContactTag" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactViewPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="DocumentCreatePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentDeletePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentEditPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentViewPermission" minOccurs="0" maxOccurs="1"/>
      </xsd:choice>
      <xsd:attribute name="securitylevel" type="DocumentSecurityLevel" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentCreatePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentDeletePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentEditPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Encapsulates the permission mappings for a given DocumentSecurityType value -->
  <xsd:element name="DocumentPermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="DocumentAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="DocumentSecurityLevel">
    <xsd:restriction base="xsd:string">
      <!-- Document accessible only by users with confidential note permissions -->
      <xsd:enumeration value="confidential"/>
      <!-- Document accessible only by users with internal note permissions -->
      <xsd:enumeration value="internalonly"/>
      <!-- Document accessible by any user in the system -->
      <xsd:enumeration value="unrestricted"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:element name="DocumentViewPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="Entity">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Account"/>
      <xsd:enumeration value="AccountContact"/>
      <xsd:enumeration value="Activity"/>
      <xsd:enumeration value="ActivityPattern"/>
      <xsd:enumeration value="Attribute"/>
      <xsd:enumeration value="AttributeUser"/>
      <xsd:enumeration value="BusinessWeek"/>
      <xsd:enumeration value="Contact"/>
      <xsd:enumeration value="Credential"/>
      <xsd:enumeration value="DirectBillMoneyRcvd"/>
      <xsd:enumeration value="Document"/>
      <xsd:enumeration value="Group"/>
      <xsd:enumeration value="GroupRegion"/>
      <xsd:enumeration value="GroupUser"/>
      <xsd:enumeration value="Holiday"/>
      <xsd:enumeration value="Note"/>
      <xsd:enumeration value="PolicyPeriod"/>
      <xsd:enumeration value="PolicyPeriodContact"/>
      <xsd:enumeration value="Producer"/>
      <xsd:enumeration value="ProducerContact"/>
      <xsd:enumeration value="Region"/>
      <xsd:enumeration value="Role"/>
      <xsd:enumeration value="RolePrivilege"/>
      <xsd:enumeration value="ScriptParameter"/>
      <xsd:enumeration value="SecurityZone"/>
      <xsd:enumeration value="SharedActivity"/>
      <xsd:enumeration value="SuspensePayment"/>
      <xsd:enumeration value="Transaction"/>
      <xsd:enumeration value="TroubleTicket"/>
      <xsd:enumeration value="User"/>
      <xsd:enumeration value="UserRole"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <!-- Encapsulates the permission mappings for a given NoteSecurityType value -->
  <xsd:element name="NoteAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="NoteCreatePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteDeletePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteEditBodyPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteEditPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteViewPermission" minOccurs="0" maxOccurs="1"/>
      </xsd:choice>
      <xsd:attribute name="securitylevel" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteCreatePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteDeletePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteEditBodyPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteEditPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Note-related permissions, allowing a mapping between actions such as view, edit, etc.
       and a required system permission based on the Note's SecurityType property -->
  <xsd:element name="NotePermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="NoteAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="NoteSecurityLevel">
    <xsd:restriction base="xsd:string">
      <!-- Note accessible only by users with confidential note permissions -->
      <xsd:enumeration value="confidential"/>
      <!-- Note accessible only by users with internal note permissions -->
      <xsd:enumeration value="internalonly"/>
      <!-- Note accessible by any user in the system -->
      <xsd:enumeration value="unrestricted"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:element name="NoteViewPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="SecurityConfig">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ContactPermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="DocumentPermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="NotePermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="StaticHandler" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="WrapHandler" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="handlerClass" type="xsd:string"/>
      <xsd:attribute name="permissionClass" type="xsd:string"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- This will create a static handler for the given permission -->
  <xsd:element name="StaticHandler">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="SystemPermType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="desc" type="xsd:string"/>
      <!-- The entity for the permission key and permision check -->
      <xsd:attribute name="entity" type="Entity" use="required"/>
      <xsd:attribute name="noPermissionDisplayKey" type="xsd:string"/>
      <!-- The permission key -->
      <xsd:attribute name="permKey" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="SystemPerm">
    <xsd:restriction base="xsd:string">
      <!-- Permission to create a new contact in the address book (Retired) -->
      <xsd:enumeration value="abcreate"/>
      <!-- Permission to add a preferred vendor to the address book (Retired) -->
      <xsd:enumeration value="abcreatepref"/>
      <!-- Permission to delete an existing contact in the address book -->
      <xsd:enumeration value="abdelete"/>
      <!-- Permission to delete an existing preferred vendor address book entry -->
      <xsd:enumeration value="abdeletepref"/>
      <!-- Permission to edit an existing contact in the address book (Retired) -->
      <xsd:enumeration value="abedit"/>
      <!-- Permission to modify an existing preferred vendor address book entry (Retired) -->
      <xsd:enumeration value="abeditpref"/>
      <!-- Permission to view and search contact entries in the address book (Retired) -->
      <xsd:enumeration value="abview"/>
      <!-- Permission to search contact entries in the address book -->
      <xsd:enumeration value="abviewsearch"/>
      <!-- Permission to bill all contracted amounts for a customer account (Retired) -->
      <xsd:enumeration value="acctbillall"/>
      <!-- Permission to change a customer's existing billing plan (Retired) -->
      <xsd:enumeration value="acctbillplanedit"/>
      <!-- Permission to view Accounts | Charges screen -->
      <xsd:enumeration value="acctchargesview"/>
      <!-- Permission to close an account -->
      <xsd:enumeration value="acctclose"/>
      <!-- Permission to add a new contact to an account -->
      <xsd:enumeration value="acctcntcreate"/>
      <!-- Permission to remove a contact from an account -->
      <xsd:enumeration value="acctcntdelete"/>
      <!-- Permission to edit information on an account contact -->
      <xsd:enumeration value="acctcntedit"/>
      <!-- Permission to view Accounts | Collateral screen -->
      <xsd:enumeration value="acctcollview"/>
      <!-- Permission to view Accounts | Contacts screen -->
      <xsd:enumeration value="acctcontview"/>
      <!-- Permission to create an account -->
      <xsd:enumeration value="acctcreate"/>
      <!-- Permission to view Accounts | Delinquencies screen -->
      <xsd:enumeration value="acctdelview"/>
      <!-- Permission to edit an existing account's details -->
      <xsd:enumeration value="acctdetedit"/>
      <!-- Permission to view Accounts | Disbursements screen -->
      <xsd:enumeration value="acctdisbview"/>
      <!-- Permission to view Accounts | Documents screen -->
      <xsd:enumeration value="acctdocview"/>
      <!-- Permission to view Accounts | Evaluation screen -->
      <xsd:enumeration value="acctevalview"/>
      <!-- Permission to view Accounts | History screen -->
      <xsd:enumeration value="accthistview"/>
      <!-- Permission to view an account regardless of the security zone it is in -->
      <xsd:enumeration value="acctignoresecurityzone"/>
      <!-- Permission to view Accounts | Invoices screen -->
      <xsd:enumeration value="acctinvcview"/>
      <!-- Permission to view Accounts | Journal screen -->
      <xsd:enumeration value="acctjournalview"/>
      <!-- Permission to view Accounts | Ledger screen -->
      <xsd:enumeration value="acctledgerview"/>
      <!-- Permission to view Accounts | Notes screen -->
      <xsd:enumeration value="acctnoteview"/>
      <!-- Permission to transfer a policy to a different account -->
      <xsd:enumeration value="acctplcytx"/>
      <!-- Permission to view Accounts | Policies screen -->
      <xsd:enumeration value="acctplcyview"/>
      <!-- Permission to view Accounts | Payments screen -->
      <xsd:enumeration value="acctpmntview"/>
      <!-- Permission to search for accounts -->
      <xsd:enumeration value="acctsearch"/>
      <!-- Permission to view Accounts | Summary screen -->
      <xsd:enumeration value="acctsummview"/>
      <!-- Permission to view the Accounts tab -->
      <xsd:enumeration value="accttabview"/>
      <!-- Permission to view Accounts | Trouble Tickets screen -->
      <xsd:enumeration value="acctttktview"/>
      <!-- Permission to view Accounts | Transactions screen -->
      <xsd:enumeration value="accttxnview"/>
      <!-- Permission to write off an amount for an account -->
      <xsd:enumeration value="acctwo"/>
      <!-- Permission to approve any approval activity even if the activity is assigned to someone
           else; the approver is still subject to authority limit restrictions -->
      <xsd:enumeration value="actapproveany"/>
      <!-- Permission to create new activities -->
      <xsd:enumeration value="actcreate"/>
      <!-- Permission to modify (edit/skip/close) activities owned by other users -->
      <xsd:enumeration value="acteditunowned"/>
      <!-- Permission to set whether an activity is mandatory (Retired) -->
      <xsd:enumeration value="actmakemand"/>
      <!-- Permission to own an activity and to see the Desktop Activities page -->
      <xsd:enumeration value="actown"/>
      <!-- Permission to create new activity patterns -->
      <xsd:enumeration value="actpatcreate"/>
      <!-- Permission to delete activity patterns -->
      <xsd:enumeration value="actpatdelete"/>
      <!-- Permission to edit activity patterns -->
      <xsd:enumeration value="actpatedit"/>
      <!-- Permission to view the list of activity patterns or activity pattern details -->
      <xsd:enumeration value="actpatview"/>
      <!-- Permission to assign an activity from a queue (Retired) -->
      <xsd:enumeration value="actqueueassign"/>
      <!-- Permission to get the next activity off of a queue (Retired) -->
      <xsd:enumeration value="actqueuenext"/>
      <!-- Permission to pick an activity from a queue (Retired) -->
      <xsd:enumeration value="actqueuepick"/>
      <!-- Permission to reassign your own activities -->
      <xsd:enumeration value="actraown"/>
      <!-- Permission to reassign activities owned by other users -->
      <xsd:enumeration value="actraunown"/>
      <!-- Permission to review and approve manually-approved assignables -->
      <xsd:enumeration value="actreviewassign"/>
      <!-- Permission to search for activities -->
      <xsd:enumeration value="actsearch"/>
      <!-- Permission to view activities -->
      <xsd:enumeration value="actview"/>
      <!-- Permission to view all activity queues, even those in other security zones (Retired) -->
      <xsd:enumeration value="actviewallqueues"/>
      <!-- Permission to create or reverse adhoc credit -->
      <xsd:enumeration value="adhoccredit"/>
      <!-- Governs use of command-line tools as well as user attribute administration (Retired) -->
      <xsd:enumeration value="admincmdline"/>
      <!-- Permission to execute the data change. -->
      <xsd:enumeration value="admindatachangeexec"/>
      <!-- Permission to view the data change page. -->
      <xsd:enumeration value="admindatachangeview"/>
      <!-- Permission to view the Admin tab -->
      <xsd:enumeration value="admintabview"/>
      <!-- Permission to create a new agency bill plan -->
      <xsd:enumeration value="agencybillplancreate"/>
      <!-- Permission to change an existing agency bill plan -->
      <xsd:enumeration value="agencybillplanedit"/>
      <!-- Permission to view agency bill plans -->
      <xsd:enumeration value="agencybillplanview"/>
      <!-- Permission to search for agency money receipts -->
      <xsd:enumeration value="agencymoneyrecdsearch"/>
      <!-- Permission to write off an amount for an agency bill producer -->
      <xsd:enumeration value="agencywo"/>
      <!-- Permission create, edit, or delete authority limit profiles -->
      <xsd:enumeration value="alpmanage"/>
      <!-- Permission to view authority limit profiles (Retired) -->
      <xsd:enumeration value="alpview"/>
      <!-- Permission to create a new contact regardless of which tag(s) it has -->
      <xsd:enumeration value="anytagcreate"/>
      <!-- Permission to delete a contact regardless of which tag(s) it has -->
      <xsd:enumeration value="anytagdelete"/>
      <!-- Permission to edit the details of a contact regardless of which tag(s) it has -->
      <xsd:enumeration value="anytagedit"/>
      <!-- Permission to view the details of a contact regardless of which tag(s) it has -->
      <xsd:enumeration value="anytagview"/>
      <!-- Permission to archive objects -->
      <xsd:enumeration value="archive"/>
      <!-- Permission to create, edit, or delete user attributes (Retired) -->
      <xsd:enumeration value="attrmanage"/>
      <!-- Permission to view the list of user attributes or attribute details (Retired) -->
      <xsd:enumeration value="attrview"/>
      <!-- Permission to create a new billing plan -->
      <xsd:enumeration value="billplancreate"/>
      <!-- Permission to change an existing billing plan -->
      <xsd:enumeration value="billplanedit"/>
      <!-- Permission to view billing plans -->
      <xsd:enumeration value="billplanview"/>
      <!-- Permission to create, edit, or delete business week -->
      <xsd:enumeration value="buswkmanage"/>
      <!-- Permission to view the list of business week -->
      <xsd:enumeration value="buswkview"/>
      <!-- Permission to transfer cash from a producer (Retired) -->
      <xsd:enumeration value="cashfromprodtx"/>
      <!-- Permission to transfer cash to a producer (Retired) -->
      <xsd:enumeration value="cashtoprodtx"/>
      <!-- Permission to transfer cash -->
      <xsd:enumeration value="cashtx"/>
      <!-- Permission to transfer cash to an account or policyperiod that is not related to the
           source account or policyperiod (Retired) -->
      <xsd:enumeration value="cashunreltx"/>
      <!-- Permission to change contact subtype -->
      <xsd:enumeration value="changecontactsubtype"/>
      <!-- Permission to place a hold on a charge -->
      <xsd:enumeration value="chargeholdcreate"/>
      <!-- Permission to release a hold on a charge -->
      <xsd:enumeration value="chargeholdrelease"/>
      <!-- Permission to create a new charge pattern -->
      <xsd:enumeration value="chargepatterncreate"/>
      <!-- Permission to edit an existing charge pattern -->
      <xsd:enumeration value="chargepatternedit"/>
      <!-- Permission to view charge patterns -->
      <xsd:enumeration value="chargepatternview"/>
      <!-- Permission to override commission rates at the charge level -->
      <xsd:enumeration value="chrgcmsnrateoverride"/>
      <!-- Permission to create a new collection agency -->
      <xsd:enumeration value="colagencycreate"/>
      <!-- Permission to remove an existing collection agency -->
      <xsd:enumeration value="colagencydelete"/>
      <!-- Permission to change an existing collection agency -->
      <xsd:enumeration value="colagencyedit"/>
      <!-- Permission to view collection agencies -->
      <xsd:enumeration value="colagencyview"/>
      <!-- Permission to disburse collateral requirement -->
      <xsd:enumeration value="collateraldisburse"/>
      <!-- Permission to drawdown collateral requirement -->
      <xsd:enumeration value="collateraldrawdown"/>
      <!-- Permission to edit a collateral requirement -->
      <xsd:enumeration value="collateralreqedit"/>
      <!-- Permission to override the commission rate (an arbitrary percentage) for a producer
           on a single policyperiod for a given role -->
      <xsd:enumeration value="commpercoride"/>
      <!-- Permission to change the commission plan assigned to a producer's producer codes -->
      <xsd:enumeration value="commplanassnedit"/>
      <!-- Permission to create a new commission plan -->
      <xsd:enumeration value="commplancreate"/>
      <!-- Permission to change an existing commission plan -->
      <xsd:enumeration value="commplanedit"/>
      <!-- Permission to override the commission plan for a producer on a single policyperiod
           for a given role -->
      <xsd:enumeration value="commplanoride"/>
      <!-- Permission to view commission plans -->
      <xsd:enumeration value="commplanview"/>
      <!-- Permission to create confidential documents -->
      <xsd:enumeration value="confdoccreate"/>
      <!-- Permission to delete confidential documents -->
      <xsd:enumeration value="confdocdelete"/>
      <!-- Permission to edit confidential documents -->
      <xsd:enumeration value="confdocedit"/>
      <!-- Permission to view confidential documents -->
      <xsd:enumeration value="confdocview"/>
      <!-- Permission to create confidential notes -->
      <xsd:enumeration value="confnotecreate"/>
      <!-- Permission to delete confidential notes -->
      <xsd:enumeration value="confnotedelete"/>
      <!-- Permission to edit confidential notes -->
      <xsd:enumeration value="confnoteedit"/>
      <!-- Permission to view confidential notes -->
      <xsd:enumeration value="confnoteview"/>
      <!-- Permission to create a new local contact (Retired) -->
      <xsd:enumeration value="ctccreate"/>
      <!-- Permission to edit an existing local contact (Retired) -->
      <xsd:enumeration value="ctcedit"/>
      <!-- Permission to view and search local contact entries (Retired) -->
      <xsd:enumeration value="ctcview"/>
      <!-- Permission to access debug tools, even when they are disabled by a configuration parameter
           (Retired) -->
      <xsd:enumeration value="debugtools"/>
      <!-- Permission to create a new delinquency plan -->
      <xsd:enumeration value="delplancreate"/>
      <!-- Permission to change a customers's existing delinquency plan (Retired) -->
      <xsd:enumeration value="delplancustedit"/>
      <!-- Permission to change an existing delinquency plan -->
      <xsd:enumeration value="delplanedit"/>
      <!-- Permission to view delinquency plans -->
      <xsd:enumeration value="delplanview"/>
      <!-- Permission to search for delinquencies -->
      <xsd:enumeration value="delsearch"/>
      <!-- Permission to create a disbursement of an amount over the amount under contract -->
      <xsd:enumeration value="disbauccreate"/>
      <!-- Permission to create a disbursement -->
      <xsd:enumeration value="disbcreate"/>
      <!-- Permission to change disbursement payee -->
      <xsd:enumeration value="disbpayeeedit"/>
      <!-- Permission to search for disbursements -->
      <xsd:enumeration value="disbsearch"/>
      <!-- Permission to add documents -->
      <xsd:enumeration value="doccreate"/>
      <!-- Permission to remove documents -->
      <xsd:enumeration value="docdelete"/>
      <!-- Permission to edit documents -->
      <xsd:enumeration value="docedit"/>
      <!-- Permission to edit or delete all documents, regardless of the permissions set on the
           individual documents -->
      <xsd:enumeration value="docmodifyall"/>
      <!-- Permission to view documents -->
      <xsd:enumeration value="docview"/>
      <!-- Permission to view all documents, regardless of the permissions set on the individual
           documents -->
      <xsd:enumeration value="docviewall"/>
      <!-- Permission to view the event messages page -->
      <xsd:enumeration value="eventmessageview"/>
      <!-- Permission to execute a general transaction -->
      <xsd:enumeration value="gentxn"/>
      <!-- Permission to view the list of global options (Retired) -->
      <xsd:enumeration value="globalopview"/>
      <!-- Permission to edit the list of global options (Retired) -->
      <xsd:enumeration value="glocalopedit"/>
      <!-- Permission to create groups -->
      <xsd:enumeration value="groupcreate"/>
      <!-- Permission to delete groups -->
      <xsd:enumeration value="groupdelete"/>
      <!-- Permission to edit groups -->
      <xsd:enumeration value="groupedit"/>
      <!-- Permission to see the user/group tree on the Administration tab (Retired) -->
      <xsd:enumeration value="grouptreeview"/>
      <!-- Permission to view details of a group (Retired) -->
      <xsd:enumeration value="groupview"/>
      <!-- Permission to search for accounts that were created by the user's groups; this permission
           is unecessary for users with the acctsearch permission -->
      <xsd:enumeration value="grpacctsearch"/>
      <!-- Permission to create, edit, and delete holidays -->
      <xsd:enumeration value="holidaymanage"/>
      <!-- Permission to view a list of holidays or holiday details -->
      <xsd:enumeration value="holidayview"/>
      <!-- Permission to create internal documents -->
      <xsd:enumeration value="intdoccreate"/>
      <!-- Permission to delete internal documents -->
      <xsd:enumeration value="intdocdelete"/>
      <!-- Permission to edit internal documents -->
      <xsd:enumeration value="intdocedit"/>
      <!-- Permission to view internal documents -->
      <xsd:enumeration value="intdocview"/>
      <!-- Permission to administer integration events -->
      <xsd:enumeration value="integadmin"/>
      <!-- Permission to access all Internal Tools -->
      <xsd:enumeration value="internaltools"/>
      <!-- Permission to create internal notes -->
      <xsd:enumeration value="intnotecreate"/>
      <!-- Permission to delete internal notes -->
      <xsd:enumeration value="intnotedelete"/>
      <!-- Permission to edit internal notes -->
      <xsd:enumeration value="intnoteedit"/>
      <!-- Permission to view internal notes -->
      <xsd:enumeration value="intnoteview"/>
      <!-- Permission to create an invoice -->
      <xsd:enumeration value="invccreate"/>
