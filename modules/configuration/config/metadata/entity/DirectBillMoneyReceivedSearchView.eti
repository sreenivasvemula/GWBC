  exportable="false"
  subpackage="search.view"
  primaryEntity="DirectBillMoneyRcvd">
  <viewEntityLink
    name="DirectBillMoneyRcvd"
    path="ID"/>
  <viewEntityLink
    name="Account"
    path="Account"/>
