    name="ReceivedDate"
    path="ReceivedDate"/>
  <viewEntityColumn
    name="Amount"
    path="Amount_amt"/>
  <viewEntityTypekey
    name="PaymentMethod"
    path="PaymentInstrument.PaymentMethod"/>
  <viewEntityColumn
    name="Token"
    path="PaymentInstrument.Token"/>
  <viewEntityColumn
    name="RefNumber"
    path="RefNumber"/>
