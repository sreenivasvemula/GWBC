<?xml version="1.0" encoding="UTF-8"?><subtype xmlns="http://guidewire.com/datamodel" desc="Billing instruction used to renew a policy" displayName="Renewal" entity="Renewal" subpackage="accounting.billinginstruction" supertype="NewPlcyPeriodBI">
