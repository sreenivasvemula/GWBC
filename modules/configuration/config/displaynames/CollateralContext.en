<?xml version="1.0" encoding="UTF-8"?>
<Entity name="CollateralContext">
  <Columns>
      <Column beanPath="CollateralContext.Subtype" name="Subtype"/>
      <DisplayNameColumn beanPath="CollateralContext.Collateral" name="Collateral"/>
  </Columns>
  <DisplayName>
     if(CollReqTransferContext.isAssignableFrom(Type.forName("entity." + Subtype))){
        return ""
     }
     return Collateral      
  </DisplayName>
</Entity>